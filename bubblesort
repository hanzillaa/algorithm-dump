#function that performs sort on a size n array
def bubblesort(array):
    n = len(array)

    for i in range(n):
        for j in range(0, n-i-1):
            #elements are swapped if element found is greater than the next element
            if array[j] > array[j+1]:
                array[j], array[j+1] = array[j+1], array[j]

array = [3,2,4,5,7,8,6,10,1,9]

print("Unsorted array is: ")
print(array)
print("Sorted array is: ")
bubblesort(array)
for i in range(len(array)):
    print("%d" % array[i], end=" ")



